@model IEnumerable<MultipleModelDataInSingleView.Models.Teacher>

<h1>TEACHERS DATA</h1>
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Gender</th>
            <th>Qualification</th>
            <th>Salary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model) 
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Gender</td>
                <td>@item.Qualification</td>
                <td>@item.Salary</td>
            </tr>
        }

    </tbody>
</table>